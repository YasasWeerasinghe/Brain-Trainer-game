<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yasasweerasinghe.mobilecw01">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher_round01"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_launcher_round01"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".PopWindow"
            android:theme="@style/AppTheme.PopUpWindowTheme" />
        <activity
            android:name=".GameLevelsWindow"
            android:screenOrientation="portrait"></activity>

        <activity
            android:name=".GameWindow"
            android:screenOrientation="portrait"></activity>

        <activity
            android:name=".Score"
            android:screenOrientation="portrait"></activity>




    </application>

</manifest>